#include<stdio.h>
#include<string.h>
int main(){
  int a,s,d,f,g,h;
  scanf("%d",&a); // x1
  scanf("%d",&s); // y1
  scanf("%d",&d);  // x2
  scanf("%d",&f); // y2
  scanf("%d",&g);  // x3
  scanf("%d",&h);  // y3
  printf("(%d , %d )\t(%d , %d )\t(%d , %d )",a,s,d,f,g,h);
  int q=s/a;
  int w=f/d;
  int e=h/g;
  if(q==w && q==e) printf("\nyes on the straight line ");
  else printf("\nnope");
  return 0;
}
