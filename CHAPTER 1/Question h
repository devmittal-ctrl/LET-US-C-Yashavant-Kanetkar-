#include<stdio.h>
int main(){
    // reversing a 5 digit number 
    int n;
    printf("enter a 5 digit number : ");
    scanf("%d",&n);
    int ans=0,rem;
    if(n>=10000 && n<=99999)
        while(n!=0){
            rem=n%10;
            ans=ans*10+rem;
            n/=10;
        }
        printf("ANSWER = %d",ans);
    return 0;
}
