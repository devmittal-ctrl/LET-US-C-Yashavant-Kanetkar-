#include<stdio.h>
int main() {
   // the report card of the student which consists the marks of 5 subjects
   float a;
   float sum=0;
   for(int i=1;i<6;i++){
    printf("enter the marks of student in %d subject \n",i);
    scanf("%f",&a);
    printf("/100");
    printf("\n");
    sum=sum+a;
   }
   printf("the percentage got by student is %f %%",(sum*100)/500);
    return 0;
}
